{
  "publishers": [
    "cjsewell",
    "ebp-bot"
  ],
  "requires_dist": {
    "dists": [
      "docutils",
      "jinja2",
      "markdown-it-py",
      "mdit-py-plugins",
      "pyyaml",
      "sphinx"
    ],
    "specifiers": [
      "docutils (>=0.15)",
      "jinja2",
      "markdown-it-py (~=0.6.0)",
      "mdit-py-plugins (~=0.2.4)",
      "pyyaml",
      "sphinx (<4,>=2)"
    ]
  },
  "requires_extras": {
    "code_style": {
      "dists": [
        "black",
        "flake8",
        "pre-commit"
      ],
      "specifiers": [
        "black; extra=='code_style'",
        "flake8 (<3.8.0,>=3.7.0); extra=='code_style'",
        "pre-commit (==1.17.0); extra=='code_style'"
      ]
    },
    "linkify": {
      "dists": [
        "linkify-it-py"
      ],
      "specifiers": [
        "linkify-it-py (~=1.0); extra=='linkify'"
      ]
    },
    "rtd": {
      "dists": [
        "ipython",
        "sphinx-book-theme",
        "sphinx-panels",
        "sphinxcontrib-bibtex"
      ],
      "specifiers": [
        "ipython; extra=='rtd'",
        "sphinx-book-theme (>=0.0.36); extra=='rtd'",
        "sphinx-panels (~=0.5.2); extra=='rtd'",
        "sphinxcontrib-bibtex (<2.0.0); extra=='rtd'"
      ]
    }
  },
  "requires_python": ">=3.6",
  "yanked": []
}
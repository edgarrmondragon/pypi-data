    "minc2": {
      "dists": [
        "h5py"
      ],
      "specifiers": [
        "h5py; extra=='minc2'"
      ]
    },
    "spm": {
      "dists": [
        "scipy"
      ],
      "specifiers": [
        "scipy; extra=='spm'"
      ]
    },
    "style": {
      "dists": [
        "flake8"
      ],
      "specifiers": [
        "flake8; extra=='style'"
      ]
    }
